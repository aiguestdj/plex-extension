{
    "name": "@aiguestdj/plex-extension",
    "version": "1.0.48",
    "description": "Plex extension to work with AI Guest DJ",
    "scripts": {
        "deploy": "npm run build && npm version patch --no-git-tag-version && npm publish",
        "docker": "PACKAGE_VERSION=$(grep '\"version\"' package.json | cut -d '\"' -f 4 | head -n 1) && docker buildx build --platform=linux/amd64,linux/arm64 -t aiguestdj/plex-extension-aiguestdj -t aiguestdj/plex-extension-aiguestdj:$PACKAGE_VERSION --push .",
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=10.0.0"
    },
    "bin": {
        "aiguestdj-plex": "./server.js"
    },
    "dependencies": {
        "@aiguestdj/shared": "^1.0.32",
        "@emotion/react": "^11.11.3",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.15.3",
        "@mui/joy": "^5.0.0-beta.21",
        "@mui/lab": "^5.0.0-alpha.134",
        "axios": "^1.4.0",
        "fetch-jsonp": "^1.3.0",
        "next": "^14.1.4",
        "next-compose-plugins": "^2.2.1",
        "next-connect": "^1.0.0",
        "notistack": "^3.0.1",
        "openai": "^4.28.0",
        "plex-api": "^5.3.2",
        "qs": "^6.12.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass": "^1.63.3",
        "sharp": "^0.33.2"
    },
    "devDependencies": {
        "@types/node": "^20.3.0",
        "@types/qs": "^6.9.14",
        "@types/react": "^18.2.11",
        "@types/react-dom": "^18.2.7",
        "eslint": "8.42.0",
        "eslint-config-next": "^14.1.4"
    }
}
